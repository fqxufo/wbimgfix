{
    "name": "wbimgfix",
    "description": "简单修复新浪微博图床",
    "version": "0.5",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "background.js"
        ]
    },

    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      },
    
     "permissions": [
        "webRequest",
        "webRequestBlocking",
        "storage",
        "http://*/*",
        "https://*/*"
    ]
}